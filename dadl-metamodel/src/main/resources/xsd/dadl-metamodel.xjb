<jxb:bindings xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:jxb="http://java.sun.com/xml/ns/jaxb"
    version="2.1" xmlns:simplify="http://jaxb2-commons.dev.java.net/basic/simplify"
    jxb:extensionBindingPrefixes="simplify">

    <jxb:bindings schemaLocation="dadl-metamodel.xsd">
        <jxb:bindings node="/xs:schema/xs:element[@name='model']/xs:complexType">
            <simplify:property name="simpleTypeOrEnumerationOrSequence">
                <simplify:as-element-property />
            </simplify:property>
        </jxb:bindings>
    </jxb:bindings>

</jxb:bindings>